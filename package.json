{
  "name": "backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prepare": "husky install",
    "commit": "cz",
    "commit:signed": "cz -S",
    "udeps": "yarn upgrade-interactive --latest",
    "migrate:dev": "dotenv -e .env.development prisma format && dotenv -e .env.development prisma migrate dev && dotenv -e .env.development prisma generate",
    "migrate:staging": "dotenv -e .env.staging prisma format && dotenv -e .env.staging prisma migrate dev && dotenv -e .env.staging prisma generate",
    "studio:dev": "dotenv -e .env.development prisma studio",
    "buildx": "sh ./buildx.sh"
  },
  "dependencies": {
    "@nestjs/apollo": "^10.1.0",
    "@nestjs/bull": "^0.6.1",
    "@nestjs/common": "^9.1.2",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.1.2",
    "@nestjs/graphql": "^10.1.2",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^9.1.2",
    "@prisma/client": "^4.3.1",
    "apollo-server-express": "^3.10.2",
    "bull": "4.5.1",
    "class-validator": "^0.13.2",
    "connect-redis": "^6.1.3",
    "express-session": "^1.17.3",
    "graphql": "^16.6.0",
    "ioredis": "^5.2.3",
    "ms": "^2.1.3",
    "nanoid": "^3.0.0",
    "passport": "^0.6.0",
    "passport-steam": "^1.0.17",
    "redis": "^4.3.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "steamcommunity": "^3.44.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.1.2",
    "@types/bull": "^3.15.9",
    "@types/connect-redis": "^0.0.18",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.5",
    "@types/jest": "29.0.3",
    "@types/ms": "^0.7.31",
    "@types/node": "^18.7.20",
    "@types/passport-steam": "^1.0.1",
    "@types/steamcommunity": "^3.43.2",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "@typescript-eslint/parser": "^5.38.0",
    "commitizen": "^4.2.5",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^8.0.1",
    "jest": "29.0.3",
    "prettier": "^2.3.2",
    "prisma": "^4.3.1",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "29.0.2",
    "ts-loader": "^9.4.1",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.1.0",
    "typescript": "^4.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog",
      "defaultScope": "global"
    }
  }
}
